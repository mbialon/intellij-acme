<idea-plugin>
  <id>net.bialon.acmeTheme</id>
  <name>IntelliJ Acme Theme</name>
  <version>0.1.0</version>
  <vendor email="marcin@bialon.net" url="https://bialon.net">Marcin Białoń</vendor>

  <description>IntelliJ Acme is a light theme for IntelliJ-based IDEs. It's inspired by the <a href="http://acme.cat-v.org/">Acme</a> text editor.</description>

  <change-notes><![CDATA[
      Add change notes here.<br>
      <em>most HTML tags may be used</em>
    ]]>
  </change-notes>

  <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <themeProvider id="42f94b1e-9c2a-4982-8d7e-99e2e91446c5" path="/acme.theme.json"/>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>